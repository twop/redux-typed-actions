{
  "name": "ts-describe-action",
  "version": "1.0.0",
  "description":
    "describe redux actions with less boilerplate and leverage typesafety",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/twop/ts-describe-action.git"
  },
  "keywords": ["redux", "typescript", "boilerplate"],
  "author": "Simon Korzunov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/twop/ts-describe-action/issues"
  },
  "homepage": "https://github.com/twop/ts-describe-action#readme",
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "skipBabel": true
      },
      "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
      "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "json", "node"]
    }
  },
  "devDependencies": {
    "@types/jest": "^22.0.1",
    "ts-jest": "^22.0.1",
    "jest": "^22.0.1",
    "typescript": "^2.6.2"
  },
  "dependencies": {}
}

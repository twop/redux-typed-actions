{
  "name": "ts-describe-action",
  "version": "2.0.1",
  "description": "describe redux actions with less boilerplate and leverage typesafety",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "test": "jest",
    "build": "tsc -d"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/twop/ts-describe-action.git"
  },
  "keywords": [
    "redux",
    "typescript",
    "boilerplate"
  ],
  "author": "Simon Korzunov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/twop/ts-describe-action/issues"
  },
  "homepage": "https://github.com/twop/ts-describe-action#readme",
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.test.json",
        "skipBabel": true
      }
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "devDependencies": {
    "@types/jest": "^23.3.9",
    "ts-jest": "^23.10.4",
    "jest": "^23.6.0",
    "typescript": "^3.1.3"
  },
  "dependencies": {}
}
